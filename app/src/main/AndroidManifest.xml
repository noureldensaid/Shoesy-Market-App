<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobile_project">

    <application
        android:allowBackup="true"
        android:icon="@drawable/shoes"
        android:label="@string/app_name"
        android:roundIcon="@drawable/shoes"
        android:supportsRtl="true"
        android:theme="@style/Theme.Mobile_project">

        <activity
            android:name=".openphoto"
            android:theme="@style/WideDialog.Transparent" />
        <activity android:name=".signin" />
        <activity android:name=".cart"
            android:theme="@style/Theme.Mobile_project1"
            />
        <activity android:name=".MainActivity">

        </activity>
        <activity android:name=".Splash_screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>